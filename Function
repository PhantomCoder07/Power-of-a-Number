#include <stdio.h>
int ans=1;
int pow (int a, int b)
{
    if (b==0)
        return 1;
    return a*pow(a,b-1);
}
int main()
{
    int base,expo;
    printf("Enter the base number: ");
    scanf("%d",&base);
    printf("Enter the exponential number: ");
    scanf("%d",&expo);
    printf("Result of %d^%d: %d\n",base,expo,pow(base,expo));
    return 0;
}
